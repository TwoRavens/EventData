  },
  "Goldstein": {
    "name": "Goldstein",
    "format": "^[+-]?\\d*\\.?\\d?+$"
  },
  "Lat": {
    "name": "Lat",
    "format": "^(|NA|[+-]?\\d*\\.?\\d+)$"
