{
  "GID": {
    "name": "GID",
    "format": "."
  },
  "Date": {
    "name": "Date",
    "format": "^\\d{8}$"
  },
  "Year": {
    "name": "Year",
    "format": "^\\d{4}$"
  },
  "Month": {
    "name": "Month",
    "format": "^([1-9]|1[0-2])$"
  },
  "Day": {
    "name": "Day",
    "format": "^([1-9]|[12]\\d|3[01])$"
