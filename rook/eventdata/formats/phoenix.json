  "Target": {
    "name": "Target",
    "format": "^(\\p{L}\\p{L}\\p{L})*$"
  },
  "TgtActor": {
    "name": "TgtActor",
    "format": "^\\p{L}\\p{L}\\p{L}$"
  },
  "TgtAgent": {
    "name": "TgtAgent",
    "format": "^(|\\p{L}\\p{L}\\p{L})$"
  },
  "TOthAgent": {
    "name": "TOthAgent",
    "format": "^(|(\\p{L}\\p{L}\\p{L}(|;))*)$"
  },
